{
    "name": "your-vendor/laravel-db-introspector",
    "description": "Generate migrations from existing database schema",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Vendor\\DbIntrospector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\DbIntrospector\\DatabaseInspectorServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "doctrine/dbal": "^3.0"
    }
}